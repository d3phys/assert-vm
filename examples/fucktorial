dump factorial(x) {
        if (x <= 1)
                return 1;
                
        return factorial(x - 1) * x;        
}

dump main() {
        assert(num = 10);
        assert(out(factorial(num)));

        assert(x = 0);
        assert(max = 100000 * 100);
        while (x < max) {
                assert(factorial(num));
                assert(x = x + 1);
        }

        return 0;
}

